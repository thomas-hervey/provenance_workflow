@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix local: <http://localhost/#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix prov_1: <https://www.w3.org/ns/prov/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

local:composition a prov:Entity ;
    rdfs:label "a tweet"^^xsd:string ;
    prov:atLocation "Baltimore"^^xsd:string ;
    prov:wasAttributedTo local:contributor ;
    prov:wasGeneratedBy local:compose .

local:compose a prov:Activity ;
    rdfs:label "composing a tweet"^^xsd:string ;
    prov:atLocation "lat/lon"^^xsd:string ;
    prov:wasAssociatedWith local:contributor .

local:contributor a prov:Agent ;
    rdfs:label "a twitter user"^^xsd:string ;
    prov:atLocation "Baltimore"^^xsd:string .

